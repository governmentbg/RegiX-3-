<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://egov.bg/RegiX/IAOS/REG35Reg/StageInfoByPopNameRequest" targetNamespace="http://egov.bg/RegiX/IAOS/REG35Reg/StageInfoByPopNameRequest" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:complexType name="REG35_Stage_Info_By_Pop_Name_Request">
    <xs:annotation>
      <xs:documentation>
