											<tr>
												<th align="left" width="250px">
													<span style="font-style:italic; font-weight:normal; ">
														<xsl:text>Съставен в</xsl:text>
													</span>
												</th>
												<td>
													<xsl:for-each select="n1:CityName">
														<xsl:apply-templates/>
													</xsl:for-each>
													<xsl:for-each select="n1:MunicipalityName">
														<span>
															<xsl:text>, общ. </xsl:text>
														</span>
														<xsl:apply-templates/>
													</xsl:for-each>
													<xsl:for-each select="n1:DistrictName">
														<span>
															<xsl:text>, обл. </xsl:text>
														</span>
