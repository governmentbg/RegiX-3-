													</xsl:for-each>
												</td>
											</tr>
											<tr>
												<th align="left">
													<span>
														<xsl:text>Област/ Населено място на администратора на лични данни</xsl:text>
													</span>
												</th>
												<td align="left">
													<xsl:for-each select="common:Location">
														<xsl:apply-templates/>
													</xsl:for-each>
												</td>
											</tr>
											<tr>
												<th align="left">
													<span>
														<xsl:text>Адрес</xsl:text>
													</span>
												</th>
												<td align="left">
													<xsl:for-each select="common:Address">
														<xsl:apply-templates/>
													</xsl:for-each>
												</td>
											</tr>
											<tr>
												<th align="left">
													<span>
														<xsl:text>Код на област/населено място</xsl:text>
													</span>
												</th>
												<td>
													<xsl:for-each select="common:LocationCode">
														<xsl:apply-templates/>
													</xsl:for-each>
												</td>
											</tr>
										</tbody>
									</table>
								</xsl:for-each>
								<br/>
							</xsl:otherwise>
						</xsl:choose>
						<br/>
					</xsl:for-each>
					<br/>
				</xsl:for-each>
