<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://www.bulstat.bg/NomenclatureEntry" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.bulstat.bg/NomenclatureEntry" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="NomenclatureEntry">
    <xs:annotation>
      <xs:documentation>Номенклатурна стойност</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="Code">
        <xs:annotation>
          <xs:documentation>Код на номенклатурна стойност</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>