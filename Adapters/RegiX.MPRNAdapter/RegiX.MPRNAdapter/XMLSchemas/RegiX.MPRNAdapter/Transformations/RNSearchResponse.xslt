															<xsl:value-of select="format-number(number(substring(string(string(.)), 6, 2)), '00')"/>
															<xsl:text>.</xsl:text>
															<xsl:value-of select="format-number(number(substring(string(string(string(.))), 1, 4)), '0000')"/>
														</span>
														<span>
															<xsl:text> г.</xsl:text>
														</span>
													</xsl:for-each>
												</td>
											</tr>
										</tbody>
									</table>
									<br/>
								</xsl:for-each>
							</xsl:when>
							<xsl:otherwise>
								<p>
									<span>
										<xsl:text>Няма данни в регистъра за вписани обстоятелства.</xsl:text>
									</span>
								</p>
							</xsl:otherwise>
						</xsl:choose>
					</xsl:for-each>
				</xsl:for-each>
