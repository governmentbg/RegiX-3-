<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <Description>The package provides information for the adapters that are part of the RegiX's core</Description>
    <Company>TechnoLogica</Company>
    <Authors>TechnoLogica</Authors>
    <Version Condition=" '$(Major)$(Minor)$(Patch)' == '' ">1.0.0.0</Version>
    <Version Condition=" '$(Major)$(Minor)$(Patch)' != '' ">$(Major).$(Minor).$(Patch).0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RegiX.Adapters.Common" Version="1.0.70" />
    <PackageReference Include="RegiX.Core.Interfaces" Version="1.0.25" />
    <PackageReference Include="RegiX.Core.Data.Interfaces" Version="1.0.13" />
    <PackageReference Include="Mono.Cecil" Version="0.11.3" />
    <PackageReference Include="System.Reflection.Metadata" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
