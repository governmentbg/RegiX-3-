<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>TechnoLogica.RegiX.Core.AdapterClientNETCore</RootNamespace>
    <Authors>Dimitar Mitev</Authors>
    <Company>TechnoLogica</Company>
    <Description>Client for NET Core Adapters. Used in the calls from the core.</Description>
    <Version Condition=" '$(Major)$(Minor)$(Patch)' == '' ">1.0.5.0</Version>
    <Version Condition=" '$(Major)$(Minor)$(Patch)' != '' ">$(Major).$(Minor).$(Patch).0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FastMember.Signed" Version="1.5.0" />
    <PackageReference Include="RegiX.Core.Data.Interfaces" Version="1.0.13" />
    <PackageReference Include="RegiX.Core.Interfaces" Version="1.0.25" />
    <PackageReference Include="System.Reflection.DispatchProxy" Version="4.7.1" />
  </ItemGroup>

</Project>
